{
  "name": "debriefr-cli",
  "description": "Run the magic debriefr from the command line",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Joel Baudin",
  "license": "MIT",
  "keywords": [
    "github",
    "reports",
    "slack"
  ],
  "scripts": {
    "watch": "nodemon src/index.ts",
    "build": "tsc",
    "start": "tsc && node lib/index.js",
    "start:local": "tsc && node -r dotenv/config lib/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "debriefr-core": "^1.0.0",
    "dotenv": "^5.0.1",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.107",
    "@types/node": "^9.6.6",
    "@types/sequelize": "^4.27.14",
    "jest": "^22.4.3",
    "nodemon": "^1.12.1",
    "ts-jest": "^22.4.4",
    "ts-node": "^4.1.0",
    "typescript": "^2.8.1"
  }
}
